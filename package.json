{
  "name": "cbor-wasm",
  "version": "0.0.0",
  "description": "CBOR decoder with the hard bits in WASM",
  "main": "lib/cbor.mjs",
  "type": "module",
  "scripts": {
    "build": "cd src; make",
    "lint": "eslint lib/*.mjs test/*.mjs",
    "pretest": "npm run build",
    "test": "node test/test.mjs",
    "pretest:browsers": "npm run build",
    "test:browsers": "node test/browsers.mjs",
    "pretest:all": "npm run build",
    "test:all": "nve 12,14,15 node test/test.mjs; node test/browsers.mjs",
    "prestart": "npm run build",
    "start": "light-server -q -s . -o /example/index.html -w 'lib/*,src/*,test/* # npm run coverage'",
    "precoverage": "npm run build",
    "coverage": "c8 -r lcov node test/test.mjs",
    "coverage:ci": "c8 -r lcov node test/test.mjs"
  },
  "keywords": [
    "cbor",
    "decoder",
    "wasm",
    "stream",
    "synchronous",
    "sync",
    "rfc8949",
    "8949"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hildjj/cbor-wasm.git"
  },
  "homepage": "https://github.com/hildjj/cbor-wasm",
  "devDependencies": {
    "@cto.af/eslint-config": "^0.0.4",
    "c8": "^7.6.0",
    "eslint": "^7.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-webassembly": "^1.11.0",
    "light-server": "^2.9.1",
    "playwright": "^1.10.0"
  },
  "engines": {
    "node": ">=12"
  }
}
