{
  "name": "cbor-wasm",
  "version": "0.0.0",
  "description": "CBOR decoder with the hard bits in WASM",
  "main": "lib/cbor.js",
  "type": "module",
  "scripts": {
    "build": "cd src; make",
    "docs": "jsdoc -c .jsdoc.conf",
    "lint": "eslint lib/*.js test/*.js",
    "pretest": "npm run build",
    "test": "npm run test:bare",
    "test:bare": "node test/test.js",
    "pretest:browsers": "npm run build",
    "test:browsers": "node test/browsers.js",
    "pretest:all": "npm run build && npm run types && npm run docs",
    "test:all": "nve --parallel 12,14,15 npm run test:bare; node test/browsers.js",
    "test:bigendian": "docker run --rm -it -v ${PWD}:/root/cbor-wasm -w /root/cbor-wasm s390x/node npm run test:bare",
    "prestart": "npm run build",
    "start": "light-server -q -s . -o /example/index.html -w 'lib/*,src/*,test/* # npm run coverage'",
    "prestart:docs": "npm run docs",
    "start:docs": "light-server -p 4002 -q -s . -o /docs/index.html -w 'README.md,.jsdoc.conf,lib/*,src/*,test/* # npm run docs && npm run types'",
    "precoverage": "npm run build",
    "coverage": "c8 -r lcov npm run test:bare",
    "coverage:ci": "c8 -r lcov npm run test:bare",
    "types": "tsc"
  },
  "keywords": [
    "cbor",
    "decoder",
    "wasm",
    "stream",
    "synchronous",
    "sync",
    "rfc8949",
    "8949"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hildjj/cbor-wasm.git"
  },
  "homepage": "https://github.com/hildjj/cbor-wasm",
  "types": "types/cbor.d.ts",
  "devDependencies": {
    "@cto.af/eslint-config": "^0.0.4",
    "better-docs": "^2.3.2",
    "c8": "^7.7.0",
    "eslint": "^7.23.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-webassembly": "^1.11.0",
    "jsdoc": "^3.6.6",
    "jsdoc-tsimport-plugin": "^1.0.4",
    "light-server": "^2.9.1",
    "playwright": "^1.10.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12"
  }
}
